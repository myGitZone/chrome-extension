{
  "name": "chrome_extension",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "antd": "^4.18.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "prettier": "^2.5.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-import": "^0.4.0"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "none",
    "semi": true,
    "arrowParens": "avoid",
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "tabWidth": 2
        }
      }
    ],
    "bracketSameLine": true
  }
}
